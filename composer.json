{
    "name": "verbruggenalex/autoalias",
    "description": "A composer package that enables auto alias generation in composer projects.",
    "require": {
        "php" : ">=5.5.0",
        "symfony/console": "~2.6",
        "symfony/yaml": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Autoalias\\Component\\Console\\": ""
        }
    },
    "scripts": {
        "post-package-install": "Autoalias\\Component\\Console\\Installer\\Installer::postInstall",
        "post-package-update": "Autoalias\\Component\\Console\\Installer\\Installer::postInstall"
    }
}
